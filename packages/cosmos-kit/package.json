{
  "name": "cosmos-kit",
  "version": "2.25.2",
  "description": "cosmos-kit wallet connector",
  "author": "developers@hyperweb.io",
  "contributors": [
    {
      "name": "Jun Liu"
    },
    {
      "name": "Tom Wang"
    },
    {
      "name": "Dan Lynch"
    },
    {
      "name": "Noah Saso"
    },
    {
      "name": "Eliot Baker"
    },
    {
      "name": "Delivan Jeonghyeok Yoo"
    }
  ],
  "homepage": "https://github.com/hyperweb-io/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "cjs",
    "esm",
    "!CHANGELOG.md",
    "!LICENSE"
  ],
  "scripts": {
    "build:cjs": "yarn tsc -p tsconfig.json --outDir cjs --module commonjs || true",
    "build:esm": "yarn tsc -p tsconfig.json --outDir esm --module es2022 || true",
    "clean:cjs": "rimraf cjs",
    "clean:esm": "rimraf esm",
    "clean": "npm run clean:cjs && npm run clean:esm",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "prepare": "npm run build",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/cosmos-kit"
  },
  "keywords": [
    "cosmos-kit",
    "cosmos",
    "keplr",
    "leap",
    "cosmostation",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/hyperweb-io/cosmos-kit/issues"
  },
  "dependencies": {
    "@cosmos-kit/bitgetwallet-extension": "^1.2.2",
    "@cosmos-kit/cdcwallet": "^2.16.3",
    "@cosmos-kit/coin98": "^2.14.3",
    "@cosmos-kit/compass": "^2.14.3",
    "@cosmos-kit/cosmostation": "^2.15.3",
    "@cosmos-kit/ctrl": "^2.14.3",
    "@cosmos-kit/exodus": "^2.13.3",
    "@cosmos-kit/fin": "^2.14.3",
    "@cosmos-kit/galaxy-station": "^2.14.3",
    "@cosmos-kit/keplr": "^2.15.3",
    "@cosmos-kit/leap": "^2.15.3",
    "@cosmos-kit/ledger": "^2.14.3",
    "@cosmos-kit/okxwallet-extension": "^2.14.3",
    "@cosmos-kit/omni": "^2.13.3",
    "@cosmos-kit/owallet": "^2.15.3",
    "@cosmos-kit/shell": "^2.14.3",
    "@cosmos-kit/tailwind": "^1.8.3",
    "@cosmos-kit/trust": "^2.14.3"
  }
}
